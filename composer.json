{
    "name": "rgilyov/trees",
    "description": "Handle trees and tries (search, traverse, build, transform)",
    "type": "library",
    "require-dev": {
        "phpunit/phpunit": "9.5.10",
        "phpbench/phpbench": "1.2"
    },
    "keywords": [
        "tree",
        "suffix tree",
        "traverse tree",
        "genealogy",
        "comments",
        "nested comments",
        "trie"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Roman Gilyov",
            "email": "rgilyov@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1"
    },
    "autoload" : {
        "psr-4" : {
            "RGilyov\\Trees\\": ["src"]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RGilyov\\Trees\\Test\\": "tests/"
        }
    },
    "minimum-stability": "dev"
}
